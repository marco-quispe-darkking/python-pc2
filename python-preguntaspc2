#problema 1

for i in range(1500,2701):
    if(i%5==0 or i%7==0):
        print(i)
#-------------------------------

#problema 2
for i in range(1,10):
    if(i<6):
        for j in range(0,i):
            print("*",end="")
    else:
        for j in range(0,10-i):
            print("*",end="")
    print()

